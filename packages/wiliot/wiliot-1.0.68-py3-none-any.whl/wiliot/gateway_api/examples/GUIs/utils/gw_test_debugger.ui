<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="ttk.Frame" id="mainwindow">
    <property name="height">250</property>
    <property name="padding">10</property>
    <property name="width">250</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Combobox" id="baud_rate_box">
        <property name="exportselection">true</property>
        <property name="state">disabled</property>
        <property name="width">10</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.17</property>
          <property name="rely">0.03</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="baud_rate_name">
        <property name="text" translatable="yes">Baud Rate:</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="padx">10</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Combobox" id="port_box">
        <property name="exportselection">true</property>
        <property name="state">disabled</property>
        <property name="takefocus">false</property>
        <property name="width">12</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.40</property>
          <property name="rely">0.03</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="port_name">
        <property name="anchor">n</property>
        <property name="text" translatable="yes">   Port:</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.35</property>
          <property name="rely">0.03</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="connect_button">
        <property cbtype="simple" name="command" type="command">on_connect</property>
        <property name="default">normal</property>
        <property name="text" translatable="yes">Connect</property>
        <property name="width">10</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="reset_button">
        <property cbtype="simple" name="command" type="command">on_reset</property>
        <property name="state">disabled</property>
        <property name="text" translatable="yes">Reset</property>
        <property name="width">10</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="rely">0.94</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="recv_box">
        <property name="autoseparators">true</property>
        <property name="blockcursor">false</property>
        <property name="height">10</property>
        <property name="relief">flat</property>
        <property name="setgrid">false</property>
        <property name="takefocus">false</property>
        <property name="undo">false</property>
        <property name="width">90</property>
        <property name="wrap">char</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">30</property>
          <property name="ipadx">150</property>
          <property name="propagate">True</property>
          <property name="row">7</property>
          <property name="sticky">nw</property>
          <property id="5" name="pad" type="row">0</property>
          <property id="7" name="pad" type="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Scrollbar" id="scrollbar">
        <property name="orient">vertical</property>
        <layout manager="grid">
          <property name="column">30</property>
          <property name="ipady">67</property>
          <property name="propagate">True</property>
          <property name="row">7</property>
          <property id="5" name="pad" type="row">0</property>
          <property id="7" name="pad" type="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Combobox" id="write_box">
        <property name="state">disabled</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">6</property>
          <property name="propagate">True</property>
          <property name="row">8</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="write_button">
        <property cbtype="simple" name="command" type="command">on_write</property>
        <property name="state">disabled</property>
        <property name="text" translatable="yes">Go</property>
        <property name="width">3</property>
        <layout manager="grid">
          <property name="column">6</property>
          <property name="propagate">True</property>
          <property name="row">8</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label1">
        <property name="font">{Segoe UI} 12 {bold}</property>
        <property name="text" translatable="yes">Gateway Test App</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">6</property>
          <property name="propagate">True</property>
          <property name="row">9</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Separator" id="separator1">
        <property name="orient">horizontal</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">30</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">ew</property>
          <property id="1" name="pad" type="row">20</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Spinbox" id="timing_profile_on">
        <property name="from_">0</property>
        <property name="increment">1</property>
        <property name="state">disabled</property>
        <property name="to">1000</property>
        <property name="width">3</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.27</property>
          <property name="rely">0.22</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Spinbox" id="timing_profile_period">
        <property name="from_">0</property>
        <property name="increment">1</property>
        <property name="state">disabled</property>
        <property name="to">1000</property>
        <property name="width">3</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.21</property>
          <property name="rely">0.22</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="set_filter">
        <property cbtype="simple" name="command" type="command">on_set_filter</property>
        <property name="offvalue">0</property>
        <property name="onvalue">1</property>
        <property name="state">disabled</property>
        <property name="text" translatable="yes">filter</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.60</property>
          <property name="rely">.22</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Spinbox" id="pace_inter">
        <property name="from_">0</property>
        <property name="increment">1</property>
        <property name="state">disabled</property>
        <property name="to">1000</property>
        <property name="width">4</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.50</property>
          <property name="rely">.22</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="config_button">
        <property cbtype="simple" name="command" type="command">on_config</property>
        <property name="state">disabled</property>
        <property name="text" translatable="yes">Set</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="pady">20</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
          <property name="sticky">n</property>
          <property id="4" name="pad" type="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Separator" id="separator3">
        <property name="orient">horizontal</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">30</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
          <property name="sticky">ew</property>
          <property id="4" name="pad" type="row">20</property>
          <property id="5" name="pad" type="row">10</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label2">
        <property name="text" translatable="yes">Energiznig
Pattern:</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label3">
        <property name="text" translatable="yes">Timing Profile [ms]
(Period/ON-Time):</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.22</property>
          <property name="rely">0.14</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label4">
        <property name="text" translatable="yes">Pacer Interval
[sec]:</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.50</property>
          <property name="rely">0.14</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label5">
        <property name="text" translatable="yes">(</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.20</property>
          <property name="rely">0.22</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label6">
        <property name="text" translatable="yes">)</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.32</property>
          <property name="rely">0.22</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label7">
        <property name="text" translatable="yes">/</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.26</property>
          <property name="rely">0.22</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Combobox" id="view_type">
        <property name="state">disabled</property>
        <property name="takefocus">false</property>
        <layout manager="grid">
          <property name="column">3</property>
          <property name="propagate">True</property>
          <property name="row">6</property>
          <property id="6" name="pad" type="row">45</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label11">
        <property name="text" translatable="yes">sample view type</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.22</property>
          <property name="rely">0.40</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="config_sum">
        <property name="background">#f2f2f2</property>
        <property name="blockcursor">false</property>
        <property name="relief">flat</property>
        <property name="setgrid">false</property>
        <property name="width">50</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="height">20</property>
          <property name="relx">0.17</property>
          <property name="rely">0.34</property>
          <property name="width">480</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label13">
        <property name="font">{Segoe UI} 10 {bold}</property>
        <property name="text" translatable="yes">Current Config:</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.00</property>
          <property name="rely">0.34</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Combobox" id="energizing_pattern">
        <property name="state">disabled</property>
        <property name="width">5</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.11</property>
          <property name="rely">0.22</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Combobox" id="received_channel">
        <property name="state">disabled</property>
        <property name="width">4</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.385</property>
          <property name="rely">0.22</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label8">
        <property name="text" translatable="yes">Received
Channel:</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.38</property>
          <property name="rely">0.14</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Combobox" id="data_type">
        <property name="state">disabled</property>
        <property name="width">10</property>
        <layout manager="grid">
          <property name="column">10</property>
          <property name="propagate">True</property>
          <property name="row">6</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label9">
        <property name="text" translatable="yes">data type</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.52</property>
          <property name="rely">0.40</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="update_button">
        <property cbtype="simple" name="command" type="command">on_update_gw_version</property>
        <property name="state">disabled</property>
        <property name="text" translatable="yes">Update GW Version to:</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.70</property>
          <property name="rely">0.0</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="clear_button">
        <property cbtype="simple" name="command" type="command">on_clear</property>
        <property name="text" translatable="yes">Clear</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">6</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="search_ports_button">
        <property cbtype="simple" name="command" type="command">on_search_ports</property>
        <property name="text" translatable="yes">refresh port</property>
        <property name="width">12</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.50</property>
          <property name="rely">0.025</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="version_path">
        <property name="state">disabled</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relwidth">0.28</property>
          <property name="relx">0.70</property>
          <property name="rely">0.06</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="version_num">
        <property name="background">#f2f2f2</property>
        <property name="height">10</property>
        <property name="relief">flat</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relheight">0.04</property>
          <property name="relwidth">0.1</property>
          <property name="relx">0.85</property>
          <property name="rely">0.005</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label10">
        <property name="text" translatable="yes">For local version update, please add the version path.
[C:\Users\user\gw_version\2.0.1.zip]</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.70</property>
          <property name="rely">0.1</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
